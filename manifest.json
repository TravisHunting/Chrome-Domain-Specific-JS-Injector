{
  "name": "0A",

  "version": "0.0.1",

  "manifest_version": 2,

  "description": "Inject Custom JS based on domain",

  "homepage_url": "http://travishunting.com",

  "background": {
    "scripts": [
      "background.js"
    ],

    "persistent": true
  },

  "browser_action": {
    "default_title": "Run JS"
  },

  "permissions": [
    "https://*/*",
    "http://*/*",
    "tabs"
  ],

  "icons": { 
    "16": "/icons/icon16.png",
    "32": "/icons/icon32.png",
    "48": "/icons/icon48.png",
   "128": "/icons/icon128.png" }

}
